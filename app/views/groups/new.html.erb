<%= simple_form_for(@group) do |f| %>
  <%= f.input :name, label: false, placeholder: 'Group name'%>
  <%= f.input :description, label: false, placeholder: 'Describe your group' %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit class: "abtn abtn-lg abtn-prime" %>
<% end %>
